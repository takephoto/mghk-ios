   pod 'MBProgressHUD', '~> 1.1.0'
   pod 'YYCache', '~> 1.0.4'
   pod 'TZImagePickerController', '1.9.8'
   pod 'ReactiveCocoa', :git => 'https://github.com/zhao0/ReactiveCocoa.git', :tag => '2.5.2'#函数响应式编程框架，配合MVVM使用该版本消除-Wreceiver-is-weak’,ignored警告
   pod 'JPush'
   #pod 'XLForm'#表单生成插件
